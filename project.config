[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/thumma532
[access "refs/for/refs/*"]
	read = group user/thumma532
	push = group user/thumma532
	label-Code-Review = -2..+2 group user/thumma532
	label-Verified = -1..+1 group user/thumma532
	rebase = group user/thumma532
	removeReviewer = group user/thumma532
	submit = group user/thumma532
[access "refs/heads/*"]
	read = group user/thumma532
	create = group user/thumma532
	push = +force group user/thumma532
	pushMerge = group user/thumma532
[access "refs/tags/*"]
	push = group user/thumma532
	push = +force group user/thumma532
